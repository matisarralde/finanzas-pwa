{
  "name": "finanzas-pwa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run generate-api-types && next build",
    "start": "next start",
    "lint": "next lint",
    "generate-api-types": "if [ -n \"$NEXT_PUBLIC_API_URL\" ]; then npx openapi-typescript $NEXT_PUBLIC_API_URL/openapi.json -o ./lib/api/schema.d.ts; else echo 'Skipping OpenAPI types generation (NEXT_PUBLIC_API_URL not set)'; fi"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.51.1",
    "@tanstack/react-query-devtools": "^5.51.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.408.0",
    "next": "14.2.5",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.3.0",
    "react": "^18",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18",
    "recharts": "^2.12.7",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "openapi-typescript": "^6.7.6",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
